alias gm="git merge"
alias push="git push"
alias pull="git pull"
alias gs="git status"
alias gb="git branch"
alias go="git checkout"
alias gc="git commit -m"
alias stash="git stash"
alias pop="git stash pop"
alias goma="git checkout master"
alias publish="npm publish"
alias desktop="cd ~/Desktop"

# aliases for github packages
